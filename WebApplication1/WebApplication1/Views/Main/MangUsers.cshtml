@using WebApplication1.Models
@model List<WebApplication1.Models.WebUserModel>
@{
    ViewBag.Title = "ניהול משתמשים";
    Layout = "~/Views/Shared/MainLayout.cshtml";
    var i = 1;
}
<style>
.center{
    width : 42px;
    height : 42px;
    display : table-cell;
    vertical-align : middle;
   
    float:none;
}

</style>
<div id="toExcel_wrapper" class="col-md-6 wrap_tab">

</div>
            <div class="row bg-primary"><h4 class="col-sm-11">&nbsp;&nbsp;&nbsp;&nbsp;@ViewBag.Title</h4><div class="col-sm-1 center"><button class="btn-default"><span class="glyphicon glyphicon-plus"></span></button></div></div>
           
        
        <table class="table tabExcel" id="toExcel">
            <thead>
                <tr>
                    <th>
                        מספר שורה
                    </th>
                    <th>
                        תעודת זהות
                    </th>
                    <th>
                        שם מלא
                    </th>
                    <th>
                        יחידה
                    </th>
                    <th>
                        תפקיד
                    </th>
                    <th>
                        סטאטוס
                    </th>
                    <th>

                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (WebUserModel Elem in Model)
                {
                    <tr>
                        <td>@i</td>
                        <td>@Elem.ID</td>
                        <td>@Elem.Name</td>
                        <td>@Elem.Unit</td>
                        <td>@Elem.Role</td>
                        <td>@Elem.Status</td>
                        <td>@Html.ActionLink("ערוך", "EditUserInMang", new { id = Elem.ID })</td>
                    </tr>
                }
            </tbody>
        </table>
     